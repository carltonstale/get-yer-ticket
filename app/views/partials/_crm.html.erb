<%= form_with model: ticket, local: true %>
<%= f.label "Customer Name: " %>
<%= f.text_field :name %> <br>

<%= f.label "Phone Number: " %>
<%= f.telephone_field :phone_number %> <br>

<%= f.label "Email: " %>
<%= f.email_field :email %>

<%= f.label "Make: " %>
<%= f.text_field :make %>

<%= f.label "Model/Series: " %>
<%= f.text_field :model_series %>

<%= f.label "Color: " %>
<%= f.text_field :color %>

<%= f.label "Size: " %>
<%= f.number_field :color %>

<%= f.submit %>
<% end %>