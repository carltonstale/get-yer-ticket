<!DOCTYPE html>
<html>
  <head>
    <title>GetYerTicket</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% flash.each do |k, v| %>
            <div><%= v %></div>
        <% end %>
    <%= yield %>
      <% if session[:user_id] %>
      <%= button_to "Log Out", logout_path, method: :get %>
      <% end %>
  </body>
</html>
